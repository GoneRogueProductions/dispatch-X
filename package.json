{
  "dependencies": {
    "@fluentui/web-components": "^2.5.16",
    "@types/node": "^20.5.9",
    "autoprefixer": "^10.4.15",
    "bootstrap": "^5.3.1",
    "browserify": "^17.0.0",
    "commonjs": "^0.0.1",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.29",
    "postcss-font-magician": "^3.0.0",
    "postcss-preset-env": "^9.1.3",
    "postcss-svgo": "^6.0.0",
    "sass": "^1.66.1"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "build": "npm run build-typescript && npm run build-browserify && npm run build-sass",
    "build-typescript": "tsc",
    "build-browserify": "browserify assets/scripts/js/fluent.js > assets/scripts/js/fluent.js",
    "doiuse": "doiuse --browsers \"ie >= 9, > 1%, last 2 versions\" assets/styles/css/style.css",
    "build-css": "gulp css",
    "build-sass-watch": "sass --watch assets/styles/scss:assets/styles/css"
  },
  "resolutions": {
    "glob-parent": "^6.0.1",
    "postcss": "^8.2.10"
  },
  "devDependencies": {
    "chokidar": "^3.5.3",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0"
  }
}
